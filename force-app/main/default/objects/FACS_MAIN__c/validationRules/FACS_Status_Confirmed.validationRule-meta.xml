<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FACS_Status_Confirmed</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL(PRIORVALUE(FACS_STATUS__c), &quot;Confirmed&quot;))</errorConditionFormula>
    <errorMessage>Confirmed record status cannot be changed.</errorMessage>
</ValidationRule>
